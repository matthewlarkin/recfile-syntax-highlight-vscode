{
	"name": "recfile-syntax",
	"displayName": "Recfile Syntax Highlighting",
	"description": "Syntax highlighting for Recfiles",
	"version": "0.0.2",
	"publisher": "your-publisher-name",
	"engines": {
	  "vscode": "^1.56.0"
	},
	"categories": [
	  "Programming Languages"
	],
	"contributes": {
	  "languages": [
		{
		  "id": "recfile",
		  "aliases": ["Recfile", "recfile"],
		  "extensions": [".rec", ".recfile"],
		  "configuration": "./language-configuration.json"
		}
	  ],
	  "grammars": [
		{
		  "language": "recfile",
		  "scopeName": "text.recfile",
		  "path": "./syntaxes/recfile.tmLanguage.json"
		}
	  ]
	}
  }
  